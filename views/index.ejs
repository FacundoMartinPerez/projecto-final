    <title>Document</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.3/font/bootstrap-icons.css">
    <link rel="stylesheet" href="./css/main.css">
</head>
<body>
<style>
* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
    font-family: sans-serif;
}

header {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 2rem;
    background-color: #ececec;
}
h1{font-size: 3.5em;}
h2{font-size: 2.7em;}
h3{font-size: 1.5em;}
p{font-size: 1.25em;} 
button{
    font-size: 1.5em;
    font-weight: bold;
    padding: 10px 20px;
    border-radius: 10px;
    border: 1px solid rgba(0,0,0,0.5);
    box-shadow: 2px 2px 10px rgba(0,0,0,0.5);
    color: white;
    background-color: rgb(57, 33, 79);
    cursor: pointer;
    
}
button:hover{
background-color: rgb(113, 40, 182);
}
.color-acento{
    color: blueviolet;
}
.container{
    max-width: 1400px;
    margin: auto;
}
header{
    background-color:  rgb(27, 27, 27);
}
.logo{
margin: 5px;
padding: 5px;
font-weight: bold;
color: rgb(255, 255, 255);
font-size: 5rem;
}

header .container{
display: flex;
flex-direction: column;
align-items: center;
}
header nav{
display: flex;
flex-direction: column;
text-align: center;
flex-direction: column;
}
header a{
    padding: 0px 0px;
    text-decoration: none;
    font-weight: bold;
    color: rgb(255, 255, 255);

}
header a:hover{
    color: blueviolet;
}
.contenedor{
    margin: 0 auto;
    width: 90%;
    max-width: 1600px;
    overflow: hidden;
    display: flex;
flex-direction: column;
align-items: center;
}
.logo {
    max-width: 5rem;
}

.nav-list {
    list-style-type: none;
    gap: 1rem;
}
.nav-list li{
display: inline-block;
padding: 0 20px;
}
.nav-list li a {
   
    text-decoration: none;
    color: #ffffff;
}
.nav-list li a:hover {
    text-decoration: none;
    color: #b72bc7;
}
.nav__logo{
    color: #ececec;
    cursor: pointer;
}
.nav__logo:hover{
    color: #8c319f;
}
.abrir-menu,
.cerrar-menu {
    display: none;
}
#hero{
    display: flex;
    align-items: center;
    justify-content: center;
    text-align: center;
    flex-direction: column;
    height: 100vh;
    
    background-image: linear-gradient(
    0deg,
    rgba(0,0,0,0.5),
    rgba(0,0,0,0.5)
    ),url("data:image/jpeg;base64,");
    background-repeat: no-repeat;
    background-size: cover;
    background-position: center center;
}
#hero h1{
    color: white;
}
#hero a{
    color:white;
}
#Nosotros{
    text-align: center;
    padding: 200px 12px;
}

#servicios{
    text-align: center;
    color: #fff;
    background-color: #1b1b1b;
}
#servicios .container {
   
    padding: 150px 12px;
    
}
#servicios h2{
    margin-top: 0;
}
#servicios p{
display: none;
}
#servicios .carta{
    background-size: cover;
    background-position: center center;
    padding: 50px 0px;
    margin: 30px;
    border-radius: 15px;


}
.carta:first-child{
    background-image: linear-gradient(
        0deg,
        rgba(0,0,0,0.5),
        rgba(0,0,0,0.5)
        ),url("Logo Empresas.jpg");
        
}
.carta:nth-child(2){
    background-image: linear-gradient(
        0deg,
        rgba(0,0,0,0.5),
        rgba(0,0,0,0.5)
        ),url("Logo Empresas.jpg");
       

}
.carta:nth-child(3){
    background-image: linear-gradient(
        0deg,
        rgba(0,0,0,0.5),
        rgba(0,0,0,0.5)
        ),url("Logo Empresas.jpg");
        

}
.carta:nth-child(4){
    background-image: linear-gradient(
        0deg,
        rgba(0,0,0,0.5),
        rgba(0,0,0,0.5)
        ),url("Logo Empresas.jpg");
        

}
#empleo{
    text-align: center;
    
    color: #fff;
    background-color: #ffffff;
}
#empleo .container{
    padding: 150px 12px;
}
#contacto{
    text-align: center;
    
    color: #fff;
    background-color: #2b2563;
}
#contacto .container{
    padding: 150px 12px;
    
}
footer {
    background-color: #333;
    color: #fff;
    height: 200px;
    padding: 10px 20px;
    text-align: center;
}
footer p{
    margin-top: 100px;
}
@media(min-width: 720px) {
#Nosotros .container {
        display: flex;
        justify-content: space-evenly;
}
#Nosotros .texto{
        width: 50%;
        max-width: 600px;
        text-align: initial;
        
        display: flex;
        justify-content: center;
        flex-direction: column;
}
    #Nosotros .img-container {
        background-image: url('data:image/jpeg;base64,');  
        background-size: cover;
